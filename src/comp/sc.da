import logging

log = logging.getLogger("SC")

class SC(process):

    def setup(rcmap:dict):
        pass

    def run():
        log.info("SC is up")
        while True:
            await(received(('req',)))

    def receive(msg=(req,), from_=p):
        log.info("Incoming request: %s", req)
        send('done', to=p)

